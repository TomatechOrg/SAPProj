<div id="<%= dom_id review %>">
  <p>
    <strong>Title:</strong>
    <%== review.title %>
  </p>

  <p>
    <strong>Body:</strong>
    <%= review.body %>
  </p>

  <p>
    <strong>User:</strong> <%= review.user.username %> <span style="display: none">( <%= review.user.email %> )</span>
  </p>

</div>
